  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "bin": {
    "tictactoe": "dist/bin/tictactoe.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/utarwyn/discord-tictactoe"
  },
  "keywords": [
    "discord",
    "bot",
