    "game",
    "nodejs",
    "typescript"
  ],
  "devDependencies": {
    "@types/i18n": "^0.12.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.5",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "discord.js": "^12.5.1",
    "i18n": "Â ^0.13.2"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "format:check": "prettier --config .prettierrc --list-different \"**/**.ts\"",
    "format": "prettier --config .prettierrc --write \"**/**.ts\"",
    "lint": "eslint . --ext .ts,.tsx",
    "serve": "node lib/replacePaths.js && node dist/bin/tictactoe.js",
    "start": "npm run build && npm run serve",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "release": "npm run clean && npm run lint && npm run test && npm run format && npm run build && node lib/replacePaths.js"
  }
}
